%h1 New messages
= form_for @message,url:user_dialog_messages_path(current_user.id,@message.dialog.id),method: :post, html: { multipart: true,class:'form' } do |f|
  %br
  = f.label 'Message'
  %br
    %p.emoji-picker-container
      = f.text_area :message_text, data: { emojiable: true }
  %br
  = f.file_field :image, id:'pictureInput'
  #target
  %br
  = f.submit 'Send',class:'btn btn-primary'
  = link_to 'Back', :back,class:'btn btn-default'