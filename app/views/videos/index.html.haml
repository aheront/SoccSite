
= form_for [@res, @video], method: :post, html: { multipart: true } do |f|
  = f.label :video
  %br
  = f.file_field :file, multiple: true,name:'video[file]'
%div#videos
  =render @videos
%script#template-upload{:type => "text/x-tmpl"}
  <div class="upload">
  {%=o.name%}
  <div class="progress">
  <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:0%"></div></div>
  </div>

