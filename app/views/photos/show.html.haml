%h1
  = @photo.name
  = link_to image_tag('loys.png'), like_user_photo_path(@user,@photo)
= (''+@photo.likes.count.to_s+' Likes')
%br
= image_tag @photo.image.url(:medium)
%br
= link_to 'Create avatar', edit_avatar_user_photo_path(@res,@photo),method: :post,class:'btn btn-danger'
- if current_user == @user
  = link_to 'Delete', url_for([@res, @photo]),method: :delete,class:'btn btn-danger'
= link_to 'Back', :back,class:'btn btn-default'